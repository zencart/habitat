[project]
	description = Vagrant Zen Cart development box
	state = hidden
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Chris Brown (drbyte)
[access "refs/for/refs/*"]
	read = group user/Chris Brown (drbyte)
	push = group user/Chris Brown (drbyte)
	label-Code-Review = -2..+2 group user/Chris Brown (drbyte)
	label-Verified = -1..+1 group user/Chris Brown (drbyte)
	rebase = group user/Chris Brown (drbyte)
	removeReviewer = group user/Chris Brown (drbyte)
	submit = group user/Chris Brown (drbyte)
[access "refs/heads/*"]
	read = group user/Chris Brown (drbyte)
	create = group user/Chris Brown (drbyte)
	push = +force group user/Chris Brown (drbyte)
	pushMerge = group user/Chris Brown (drbyte)
[access "refs/tags/*"]
	push = +force group user/Chris Brown (drbyte)
	pushTag = group user/Chris Brown (drbyte)
	pushSignedTag = group user/Chris Brown (drbyte)
